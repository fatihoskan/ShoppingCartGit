networks:
  shopping-cart-backend:
    external: true
    name: shopping-cart-backend
services:
  db:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: 1Secure*Password1#
    image: mcr.microsoft.com/mssql/server:2017-latest
  seq.logging:
    container_name: seq.logging
    environment:
      ACCEPT_EULA: "Y"
    image: datalust/seq
    ports:
    - published: 5341
      target: 80
  shoppingcart:
    build:
      context: C:\LocalProjects
      dockerfile: ShoppingCart/Dockerfile
    depends_on:
      db:
        condition: service_started
    image: shoppingcart
    networks:
      shopping-cart-backend: {}
    ports:
    - target: 80
version: '3.4'